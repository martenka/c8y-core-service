  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "docker:start": "docker-compose -f docker-compose.yml up",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "fix": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix && yarn format",
    "test": "eslint src/** && tsc --project ./tsconfig.json && jest --maxWorkers 50%",
    "test:watch": "jest --watch --maxWorkers 50%",
    "test:cov": "jest --coverage --maxWorkers 50%",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand "
  },
  "dependencies": {
    "@golevelup/nestjs-modules": "^0.7.0",
    "@golevelup/nestjs-rabbitmq": "^4.0.0",
    "@juicyllama/nestjs-redoc": "^2.3.10",
    "@nestjs/common": "^10.1.0",
    "@nestjs/config": "^3.0.0",
    "@nestjs/core": "^10.1.0",
    "@nestjs/jwt": "^10.1.0",
    "@nestjs/mongoose": "^10.0.1",
    "@nestjs/passport": "^10.0.0",
    "@nestjs/platform-express": "^10.1.0",
    "@nestjs/swagger": "^7.1.2",
    "bcrypt": "^5.1.0",
    "cron-parser": "^4.8.1",
    "human-interval": "^2.0.1",
    "mongoose": "^6.11.4",
    "nest-typed-config": "^2.7.0",
    "node-fetch": "^2.6.12",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.1.10",
    "@nestjs/schematics": "^10.0.1",
    "@nestjs/testing": "^10.1.0",
    "@types/amqplib": "^0.10.1",
    "@types/bcrypt": "^5.0.0",
    "@types/express": "^4.17.13",
    "@types/jest": "^29.5.3",
    "@types/node": "^18.11.9",
    "@types/passport-jwt": "^3.0.9",
    "@types/passport-local": "^1.0.35",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^6.1.0",
    "@typescript-eslint/parser": "^6.1.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "eslint": "^8.45.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^5.0.0",
    "jest": "^29.6.1",
    "mongodb-memory-server": "^8.13.0",
    "prettier": "^3.0.0",
    "rimraf": "^5.0.1",
    "source-map-support": "^0.5.20",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-loader": "^9.4.4",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.6"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
